{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "koa2",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/server.ts",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "outFiles": ["${workspaceRoot}/dist/**/*.js"],
      "runtimeExecutable": "nodemon",
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "restart": true
    }
  ]
}
